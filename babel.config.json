{
  "presets": 
  [
    [
      "@babel/preset-env",
      {
        "targets": 
        {
          "browsers": 
          [
            ">0.25%",
            "not ie 11",
            "not op_mini all"
          ]
        },
        "modules": false
      }
    ], 
    [
      "@babel/preset-react",
      {
        "runtime": "classic",
        "throwIfNamespace": true
      }
    ],
    [
      "@babel/preset-typescript", 
      {
        "allExtensions": true,
        "isTSX": true,
        "optimizeConstEnums": true
      }
    ]
  ],
  "ignore": 
  [
    "**/*.d.ts",
    "**/*.icloud"
  ]
}